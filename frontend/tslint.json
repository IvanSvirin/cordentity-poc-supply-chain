{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "await-promise": true,
        "no-return-await": true,
        "no-eval": true,
        "no-empty": true,
        "quotemark": [true, "single"],
        "no-console": false,
        "semicolon": false,
        "trailing-comma": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}
